<!DOCTYPE html>
<html>
  <%= render 'layouts/head' %>
  <body>
    <div class="container">
      <% flash.each do |type, message| %>
        <% content_tag :div, message, class: "alert alert#{type}" %>
      <% end %>
      <%= yield %>
    </div>
    <%= debug(params) if Rails.env.development? %>
  </body>
</html>
